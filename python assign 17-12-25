    {
      "cell_type": "code",
      "source": [
        "num = int(input(\"Enter a number: \"))\n",
        "\n",
        "even_sum = 0\n",
        "odd_sum = 0\n",
        "\n",
        "while num > 0:\n",
        "    digit = num % 10\n",
        "    if digit % 2 == 0:\n",
        "        even_sum += digit\n",
        "    else:\n",
        "        odd_sum += digit\n",
        "    num //= 10\n",
        "\n",
        "print(\"Sum of Even Digits:\", even_sum)\n",
        "print(\"Sum of Odd Digits:\", odd_sum)\n",
        "\n"
      ],
